{
  "manifest_version": 2,
  "name": "HTTP request",
  "version": "0.1",
  "background":
   {
    "scripts": ["background.js"]
  },
  "content_scripts":
  [
    {
      "matches":
      ["<all_urls>"],
      "permissions": ["https://en.wikipedia.org"],

      "js":["jquery-3.1.1.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "a.png"
  }
}
